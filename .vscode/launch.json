{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Server",
            "type": "debugpy",
            "request": "launch",
            "module": "uvicorn",
            "args": [
                "server.main:app",
                "--reload",
                "--port=6165"
            ],
            "jinja": true,
            "justMyCode": false,
        },
        {
            "type": "chrome",
            "request": "launch",
            "name": "Chrome",
            "url": "http://localhost:6166",
            "webRoot": "${workspaceFolder}"
        }
    ]
}